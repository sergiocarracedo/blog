---
import { getTags } from '../../utils/getTags';

export async function getStaticPaths() {
  const tags = await getTags();

  const paths = [];
  for (const [slug] of tags) {
    paths.push({
      params: { tag: slug },
      rewrite: `/tags/${slug}`,
    });
  }
  return paths;
}

const { tag } = Astro.params;
console.log('----', tag);
return Astro.rewrite(`/tags/${tag}/1`);
