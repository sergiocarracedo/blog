{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "paths": {
      "@/*": ["src/*", "tailwind.config.ts"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
